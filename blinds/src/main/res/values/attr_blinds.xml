<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BlindsLayout">
        <attr format="dimension" name="blindsExtraSpace"/>
        <attr format="integer" name="blindsMaxCount"/>
        <attr format="fraction" name="blindsFraction"/>
        <attr format="boolean" name="blindsFold"/>
        <attr format="enum" name="blindsFoldOrientation">
            <enum name="left" value="0"/>
            <enum name="right" value="1"/>
        </attr>
    </declare-styleable>
</resources>